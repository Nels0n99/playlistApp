extends ../layout

block content
    div.content-container
        h3 Users
        table
            thead
                tr
                    th First Name
                    th Last Name
                    th Email
            tbody
                each user in users
                    tr
                        td= user.first_name
                        td= user.last_name
                        td= user.email
                        td
                            a.button.delete(href=`/users/delete/${user.id}`) Delete User